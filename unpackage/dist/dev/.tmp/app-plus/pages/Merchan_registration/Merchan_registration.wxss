}
.div_photo .add_img {
	width: 100%;
	height: 140.83rpx;
	padding-top: 34.72rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
}
.div_photo .add_img image {
	width: 100rpx;
	height: 100rpx;
	margin-left: 2vw;
}
.qualifications_page .div input {
	width: 93vw;
}
.uni-list-cell {
	margin-right: 10vw;
}
.uni-list-cell view {
	display: inline-block;
}
.sex radio-group {
	display: inline-block;
}
.tab {
	padding: 20rpx 0;
	font-size: 32rpx;
}
.tab.active {
	color: #f00;
}
.infor_box>view  .getaddress{
	display: inline-block;
	width: 65%;
	color:#666;
}
.infor_box .address{
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-pack: start;
	-webkit-justify-content: flex-start;
	    -ms-flex-pack: start;
	        justify-content: flex-start;
	-webkit-box-align: center;
	-webkit-align-items: center;
	    -ms-flex-align: center;
	        align-items: center;
}
.basic_content .cat_text{
	width: 60vw;
	color:#666
}

