<view bind:__l="__l"><view class="navigation" style="{{'height:'+(height)+';'}}"><navigator class="left" open-type="navigateBack" hover-class="none"><image src="../../static/Merchan_registration/back.png" mode></image></navigator><view class="title">添加银行卡</view><view class="right"></view></view><view class="content" style="{{'padding-top:'+(height)+';'}}"><view class="div"><text class="title">持卡人:</text><input type="text" placeholder="请输入持卡人姓名" data-event-opts="{{[['input',[['__set_model',['$0','name','$event',[]],['']]]]]}}" value="{{name}}" bindinput="__e"/></view><view class="div"><text class="title">银行卡账号:</text><input type="text" placeholder="请输入银行卡账号" data-event-opts="{{[['input',[['__set_model',['$0','card_num','$event',[]],['']]]]]}}" value="{{card_num}}" bindinput="__e"/></view><view class="div"><text class="title">身份证号:</text><input type="text" placeholder="请输入身份证号" data-event-opts="{{[['input',[['__set_model',['$0','user_num','$event',[]],['']]]]]}}" value="{{user_num}}" bindinput="__e"/></view><view class="div"><text class="title">手机号码:</text><input type="text" placeholder="请输入手机号码" data-event-opts="{{[['input',[['__set_model',['$0','phone_num','$event',[]],['']]]]]}}" value="{{phone_num}}" bindinput="__e"/></view><view class="div"><text class="title">短信验证:</text><input type="text" placeholder="请输入短信验证码" data-event-opts="{{[['input',[['__set_model',['$0','note','$event',[]],['']]]]]}}" value="{{note}}" bindinput="__e"/><button class="{{['getNumber',(tg)?'active':'']}}" disabled="{{tg}}" data-event-opts="{{[['tap',[['getNumber',['$event']]]]]}}" bindtap="__e">{{s}}</button></view><button class="add" type="primary" data-event-opts="{{[['tap',[['add',['$event']]]]]}}" bindtap="__e">确认添加</button></view></view>