<view bind:__l="__l"><view class="navigation" style="{{'height:'+(height)+';'}}"><navigator class="left" open-type="navigateBack" hover-class="none"><image src="../../static/Merchan_registration/back.png" mode></image></navigator><view class="title">提现</view><view class="right"></view></view><view class="content" style="{{'padding-top:'+(height)+';'}}"><view class="top"><view class="title">提现金额(手续费2%)</view><view class="money"><text>￥</text><input type="text" focus="true" data-event-opts="{{[['input',[['__set_model',['$0','money','$event',[]],['']]]]]}}" value="{{money}}" bindinput="__e"/></view><view class="tip"><text>{{"可提现金额 ￥"+allmoney}}</text><text data-event-opts="{{[['tap',[['getAllmoney',['$event']]]]]}}" class="red" bindtap="__e">全部提现</text></view></view><view class="mode"><view class="mode_list"><view class="alipay"><image src="../../static/zfb.png" mode></image><text>支付宝</text></view><view data-event-opts="{{[['tap',[['zfb']]]]}}" class="choice" bindtap="__e"><image src="{{key==-1?xz:wxz}}" mode></image></view></view><block wx:for="{{card}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="mode_list"><view class="alipay"><image src="../../static/zfb.png" mode></image><text>{{"建设银行("+item.bankCardEnd+")"}}</text></view><view data-event-opts="{{[['tap',[['choice',[index]]]]]}}" class="choice" bindtap="__e"><image src="{{key==index?xz:wxz}}" mode></image></view></view></block></view><view class="bind"><navigator url="./bindCard" hover-class="none">添加银行卡</navigator></view><button class="btn" type="primary" data-event-opts="{{[['tap',[['extract']]]]}}" bindtap="__e">立即提现</button></view></view>