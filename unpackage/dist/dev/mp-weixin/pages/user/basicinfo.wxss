.mask {
	width: 100vw;
	height: 100vh;
	position: fixed;
	background: rgba(0, 0, 0, 0.4);
	top: 0;
	z-index: 9999;
}
.norms {
	width: 80vw;
	background: #fff;
	position: fixed;
	top: 50%;
	left: 50%;
	-webkit-transform: translate(-50%, -50%);
	    -ms-transform: translate(-50%, -50%);
	        transform: translate(-50%, -50%);
	border-radius: 10px;
	padding: 30rpx;
}
.norms .title {
	font-size: 34rpx;
