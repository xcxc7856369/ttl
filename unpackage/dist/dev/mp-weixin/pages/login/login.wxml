<view class="content" style="background-image:url(../../static/login/register_bg.png);" bind:__l="__l"><view class="box"><view class="reg_view"><view class="title"><image src="../../static/login/reg_phone.png" alt></image></view><input class="reg_input" placeholder="请输入手机号" placeholder-style="color:#fff;" data-event-opts="{{[['input',[['__set_model',['$0','phone','$event',[]],['denglu']]]]]}}" value="{{denglu.phone}}" bindinput="__e"/></view><view class="reg_view"><view class="title"><image src="../../static/login/reg_password.png" alt></image></view><input class="reg_input" placeholder="请输入密码" placeholder-style="color:#fff;" password="{{denglu.imgtype}}" data-event-opts="{{[['input',[['__set_model',['$0','password','$event',[]],['denglu']]]]]}}" value="{{denglu.password}}" bindinput="__e"/><view class="pw_view"><image class="codeimg" src="{{denglu.pwtype}}" data-event-opts="{{[['tap',[['pwshow',['$event']]]]]}}" bindtap="__e"></image></view></view><view class="forget_view"><view data-event-opts="{{[['tap',[['forgetfuc',['$event']]]]]}}" class="forget_a" bindtap="__e">忘记密码？</view></view><view class="btn_view"><button data-event-opts="{{[['tap',[['login_tap',['$event']]]]]}}" class="login" bindtap="__e">登录</button></view><view class="btn_view"><button data-event-opts="{{[['tap',[['reg_page',['$event']]]]]}}" class="register" bindtap="__e">新用户注册</button></view></view></view>