
















/*每个页面公共css */
page {
	font-family: 'PingFang-SC-Regular';
}
.navigation {
	width: 100%;
	color:#fff;
	background: #ED721F;
	position: fixed;
	z-index: 999999;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	line-height: 80px;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	    -ms-flex-pack: justify;
	        justify-content: space-between;
	text-align: center;
}
.navigation .left{
	width: 20%;
	height: 100%;
}
.navigation .title{
	width: 60%;
}
.navigation .right{
	width: 20%;
}
.navigation image {
	width: 16rpx;
	height: 30rpx;
	vertical-align: middle;
}
.navigation{
	color: #fff;
/* 	display: block; */
	font-size: 30.55rpx;
}
@font-face {
	font-family: 'iconfont';
	src: url(data:font/ttf;base64,) format('truetype');
}

/*每个页面公共css */
.ttl_left_iconfont {
	font-family: 'iconfont';
	color: #FFFFFF;
	font-size: 27px;
	min-width: 1em;
	margin-left: 5px;
	display: inline-block;
}
.iconfont {
	font-family: 'iconfont';
	color: #FFFFFF;
	font-size: 27px;
	min-width: 1em;
	margin-left: 5px;
	display: inline-block;
}
.ttl_view {
	width: 100%;
	position: fixed;
	top: 0;
	left: 0;
	z-index: 9999999;
}
.ttl_status_bar {
	width: 100%;
	height: 25px;
	background-color: #ED721F;
}
.ttl_head {
	width: 100%;
	height: 44px;
	background-color: #ED721F;
	line-height: 44px;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	    -ms-flex-pack: justify;
	        justify-content: space-between;
}
.ttl_left {
	width: 20%;
}
.ttl_head_title {
	width: 60%;
	font-weight: 700;
	text-align: center;
	overflow: hidden;
	white-space: nowrap;
	-o-text-overflow: ellipsis;
	   text-overflow: ellipsis;
	color: #FFFFFF;
	font-size: 16px;
}
.ttl_right {
	width: 20%;
}
.ttl_right_btn {
	color: #fff;
	min-width: 1em;
	text-align: center;
	font-size: 16px;
}

