page{
		background-color: #fff;
}
.chunck{
	width:100%;
	height:0.5rem;
	background:rgba(238,238,238,1);
}
.title{
	width:100%;
	height:1rem;
	font-size:27rpx;
	font-weight:bold;
	color:rgba(51,51,51,1);
	line-height:1rem;
	margin-top: 2.5rem;
	text-align: center;
}
.pw{
	width: 19rem;
	margin:1.5rem auto;
	position: relative;
	height:2.5rem;
}
.input_box{
	width: 19rem;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	    -ms-flex-pack: justify;
	        justify-content: space-between;
}
.input_box view{
	border:1px solid #E5E5E5;
	width: 2.5rem;
	height:2.5rem;
	border-radius: 0.2rem;
	text-align: center;
	line-height: 2.5rem;
}
.v_input{
	position:absolute;
	top:0;
	left: 0;
	width:100%;
	height: 2.5rem;
	opacity: 0;
	z-index: 10;
}
.v_input input{
	width: 100%;
	height: 100%;
}


